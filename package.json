{
  "name": "tinyflux",
  "version": "0.2.0",
  "description": "tinyflux is a flux like expect very tiny framekwork",
  "main": "src/index.js",
  "scripts": {
    "start" : "npm run build:umd && npm run build:umd:min",
    "build:umd": "webpack src/tinyflux.js dist/tinyflux.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/tinyflux.js dist/tinyflux.min.js --config webpack.config.production.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fishedee/TinyFlux.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fishedee/TinyFlux/issues"
  },
  "homepage": "https://github.com/fishedee/TinyFlux#readme",
  "devDependencies": {
    "babel-loader": "^5.3.2",
    "webpack": "^1.12.2"
  },
  "npmName": "tinyflux",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
